package application;

public class Edges {
	
	private double distance ; 
	private Node node1 ; 
	private Node node2 ; 
	


	public Edges() {
		super(); 
		// TODO Auto-generated constructor stub
	}
	
	public Edges(double distance , Node node1 , Node node2 ) {
		this.distance = distance ; 
		this.node1 = node1 ; 
		this.node2 = node2 ; 
	}
	
	@Override
	public String toString() {
		return "Edgs [Distance=" + distance + ", SourceCity=" + node1 + ", TargetCity=" + node2 + "]";
	}

	public double getDistance() {
		return distance;
	}

	public void setDistance(double distance) {
		this.distance = distance;
	}

	public Node getNode1() {
		return node1;
	}

	public void setNode1(Node node1) {
		this.node1 = node1;
	}

	public Node getNode2() {
		return node2;
	}

	public void setNode2(Node node2) {
		this.node2 = node2;
	}
	
	
	

	

}
